{
  "name": "Reading Buddy",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Stay on top of ur shit",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "notifications",
    "background"
  ]
}
